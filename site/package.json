{
  "name": "murphyl.com",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.21.4",
    "classnames": "^2.3.1",
    "gray-matter": "^4.0.3",
    "highlight.js": "^11.2.0",
    "markdown-to-jsx": "^7.1.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-json-schema": "^1.2.2",
    "react-router-dom": "^5.3.0",
    "recoil": "^0.4.1",
    "simple-icons": "^5.14.0"
  },
  "scripts": {
    "markdown": "copyfiles -f src/data/docs/**/*.md public/data/markdown",
    "topic": "copyfiles -f src/data/topics.toml src/data/**/*.toml public/data/toml/topic",
    "snippet": "copyfiles -f src/data/snippets.toml src/data/**/*.toml public/data/toml/snippet",
    "prestart": "npm run markdown && npm run topic && npm run snippet",
    "prebuild": "npm run markdown && npm run topic && npm run snippet",
    "start": "react-scripts start",
    "build": "react-scripts build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@iarna/toml": "^2.2.5",
    "copyfiles": "^2.4.1",
    "react-scripts": "3.4.1"
  }
}
