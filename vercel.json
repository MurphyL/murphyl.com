{
	"version": 2,
	"trailingSlash": true,
	"routes": [{ 
		"handle": "filesystem" 
	}, {
		"src": "/.*",
		"dest": "/index.html",
		"headers": { 
			"Cache-Control": "max-age=3600",
			"X-Site-Owner": "murphyl@outlook.com"
		}
	}, {
      "source": "/post/.*",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "max-age=600"
        }
      ]
    }]
}