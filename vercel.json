{
	"version": 2,
	"name": "murphyl-dot-com",
	"routes": [{ 
		"handle": "filesystem" 
	}, {
		"src": "/.*",
		"dest": "/index.html",
		"headers": { 
			"Cache-Control": "max-age=3600",
			"X-Site-Owner": "murphyl@outlook.com"
		}
	}, {
		"src": "/api/(.*)",
		"headers" : [{
          "key" : "X-FUNC-OWNER",
          "value" : "murphyl@outlook.com"
        }, {
          "key" : "Access-Control-Allow-Origin",
          "value" : "*"
        }, {
          "key" : "Access-Control-Allow-Methods",
          "value" : "POST"
        }, {
          "key" : "Access-Control-Allow-Headers",
          "value" : "X-Requested-With"
        }]
	}],
	"functions": {
	    "api/*.js": {
			"memory": 1024,
			"maxDuration": 10
	    }
	}
}