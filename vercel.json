{
	"version": 2,
	"name": "murphyl-dot-com",
	"routes": [{ 
		"handle": "filesystem" 
	}, { 
		"src": "/redirect", 
		"status": 308, 
		"headers": { 
			"Location": "https://git.io/JUWmP" 
		} 
	}, {
		"src": "/.*",
		"dest": "/index.html",
		"headers": { 
			"Cache-Control": "max-age=3600",
			"X-Site-Owner": "murphyl@outlook.com"
		}
	}],
	"functions": {
	    "api/*.js": {
			"memory": 1024,
			"maxDuration": 10
	    }
	}
}