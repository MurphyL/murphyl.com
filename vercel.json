{
	"version": 2,
	"name": "murphyl-dot-com",
	"routes": [{ 
		"handle": "filesystem" 
	}, {
		"src": "/.*",
		"dest": "/index.html",
		"headers": { 
			"Cache-Control": "max-age=3600",
			"X-Site-Owner": "murphyl@outlook.com"
		}
	}],
	"redirects": [{ 
		"source": "/presets/vim", 
		"destination": "https://git.io/JUWmP" 
	}],
	"functions": {
	    "api/desc.js": {
			"memory": 1024,
			"maxDuration": 10
	    },
		"api/github.js": {
			"memory": 1024,
			"maxDuration": 10
	    },
		"api/contact.js": {
			"memory": 1024,
			"maxDuration": 10
	    },
		"api/gh_v4.js": {
			"memory": 1024,
			"maxDuration": 10
	    }
	}
}